        <dc:title>The Unincorporated Man</dc:title>
        <dc:creator opf:file-as="Kollin, Dani &amp; Kollin, Eytan" opf:role="aut">Dani Kollin</dc:creator>
        <dc:creator opf:file-as="Kollin, Dani &amp; Kollin, Eytan" opf:role="aut">Eytan Kollin</dc:creator>
